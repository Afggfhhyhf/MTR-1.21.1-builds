{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"javaImplements": [
		"SerializedDataBase"
	],
	"properties": {
		"id": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"color": {
			"type": "string"
		},
		"transportMode": {
			"$ref": "TransportMode"
		},
		"length": {
			"type": "number",
			"minimum": 0
		},
		"width": {
			"type": "number",
			"minimum": 0
		},
		"bogie1Position": {
			"type": "number"
		},
		"bogie2Position": {
			"type": "number"
		},
		"description": {
			"type": "string"
		},
		"wikipediaArticle": {
			"type": "string"
		},
		"models": {
			"type": "array",
			"items": {
				"$ref": "vehicleModel.json"
			}
		},
		"bogie1Models": {
			"type": "array",
			"items": {
				"$ref": "vehicleModel.json"
			}
		},
		"bogie2Models": {
			"type": "array",
			"items": {
				"$ref": "vehicleModel.json"
			}
		},
		"bveSoundBaseResource": {
			"type": "string"
		},
		"speedSoundCount": {
			"type": "integer",
			"minimum": 1
		},
		"speedSoundBaseResource": {
			"type": "string"
		}
	},
	"required": [
		"id",
		"name",
		"color",
		"length",
		"width",
		"bogie1Position",
		"bogie2Position",
		"description",
		"wikipediaArticle",
		"bveSoundBaseResource",
		"speedSoundCount",
		"speedSoundBaseResource"
	]
}
